{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:import/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
},
  "rules": {
      "prettier/prettier": [
          "error",
          {
              "parser": "flow"
          }
      ],
      "semi": ["error", "always"],
      "no-multi-spaces": ["error"],
      "import/named": 2,
      "import/namespace": 2,
      "import/default": 2,
      "import/export": 2
  }
}
